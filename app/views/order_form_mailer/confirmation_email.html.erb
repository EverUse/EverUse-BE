        word-break: break-word;
        text-align: center;
        mso-line-height-alt: 17px;
        margin: 0;
      }

      .divider-container {
        word-break: break-word;
        vertical-align: top;
        min-width: 100%;
        padding: 0;
        -ms-text-size-adjust: 100%;
        -webkit-text-size-adjust: 100%;
      }

      .divider-table {
        table-layout: fixed;
        vertical-align: top;
        border-spacing: 0;
        border-collapse: collapse;
        mso-table-lspace: 0pt;
        mso-table-rspace: 0pt;
        border-top: 0px solid transparent;
        min-width: 100%;
        -ms-text-size-adjust: 100%;
        -webkit-text-size-adjust: 100%;
      }

      .divider-row {
        vertical-align: top;
      }

      .divider-cell {
