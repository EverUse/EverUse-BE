        margin: 0 auto;
        background-color: transparent;
        border-collapse: collapse;
        display: table;
        width: 100%;
      }

      .block-grid.special_color{
        background-color: #ffffff;
      }

      .col{
        min-width: 320px;
        max-width: 680px;
        display: table-cell;
        vertical-align: top;
        width: 680px;
      }

      .col.product_input{
